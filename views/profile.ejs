<%- include('partials/header') %>
<div class="container-fluid dash-head">
    <nav class="navbar navbar-expand-lg navbar-light sticky-top" style="background-color: #fecea8;">
        <a class="navbar-brand logo" href="/dashboard">NoStoryGram</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
            aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
            <div class="navbar-nav">

                <a class="nav-item nav-link" href="/dashboard">Home</a>
                <a class="nav-item nav-link active" href="#">Profile<span class="sr-only">(current)</span></a>
                <a class="nav-item nav-link" href="/upload">Upload</a>
                <a class="nav-item nav-link" href="/logout" role="button">Logout</a>




            </div>
        </div>
    </nav>
</div>
<hr>
<div class="container-fluid bio">
    <div class="row">
        <div class="col-4 dp-row">
            <img src="./uploads/<%= dp %>" class="dp" alt="">
        </div>
        <div>
          <strong><h1 class="pro-name"><%= name %></h1></strong>
            <h5 class="pro-bio"><%= bio %></h5>
        </div>
 
    </div>
   
    


</div>
<div class="container photo-area ">
    <div class="row column-area">
        
    
        <%  userPosts.slice().reverse().forEach(function(userPost){ %>
            <div class="col-4 column post-tile">
            <img src="./uploads/<%=userPost.img%>" class="profile-photo" alt="">
            </div>
        
        <% }) %>
    
   
    
    </div>
</div>






<%- include('partials/footer') %>